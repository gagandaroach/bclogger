<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
            xmlns:app="http://schemas.android.com/apk/res-auto"
            xmlns:tools="http://schemas.android.com/tools" android:id="@+id/nav_graph"
            app:startDestination="@+id/dashboard">

  <fragment android:id="@+id/dashboard" android:name="tech.daroach.bclogger.ui.Dashboard"
            android:label="fragment_dashboard" tools:layout="@layout/fragment_dashboard">
    <action
        android:id="@+id/action_dashboard_to_new_cycle_graph"
        app:destination="@id/new_cycle_graph"
        app:exitAnim="@anim/nav_default_exit_anim" />
  </fragment>

  <include app:graph="@navigation/new_cycle_graph" />


</navigation>